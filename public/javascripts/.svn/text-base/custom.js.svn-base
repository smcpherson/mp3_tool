function hilight(e) {e.className += ' hilite';}
function delight(e) {e.className = e.className.replace('hilite','');}
function classOn(e,c) {e.className += ' '+c;}
function classOff(e,c) {e.className = e.className.replace(c,'');}
function show(e){alert(e); var x = document.getElementById(e); x.style.display = '';}
function hide(e){var x = document.getElementById(e); x.style.display = 'none';}
function toggle(e){var x = document.getElementById(e); if (x.style.display == 'none') {show(e)} else {hide(e)}}
function folderNameClicked(e){x = 'folder_'+e+'_contents'; new Effect.Highlight(x);}
function matchedAlbumShowTracksClicked(e){x = 'tracks_'+e; new Effect.Highlight(x);}
function matchedAlbumShowTracksClicked(e){x = 'tracks_'+e; new Effect.Highlight(x);}
function startClicked(){x = $('main_folder_listing'); x.innerHTML="<p>Please wait -- Loading Folders...</p>"}
function folderClicked(i){Effect.toggle('folder_'+i+'_files'); return false;}
